@model IEnumerable<TurnosApp.Models.Specialty>


<table>
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Description)</th>              
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Description)</td>
            </tr>
        }            
    </tbody>
</table>
